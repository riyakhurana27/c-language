#include<stdio.h>
#include<conio.h>
int main()
{
    int a[10],i,s=0,e=0;
    printf("Enter 10 number");
    for(i=0;i<=9;i++)
     scanf("%d",&a[i]);
     for(i=0;i<=9;i++)
     {
        if(a[i]%2==0)
        {
        e=e+a[i];
        }
        else

        s=s+a[i];

     }
     printf(" Sum of even num is %d",e);
     printf("sum of odd number is %d",s);
     return 0;

}
